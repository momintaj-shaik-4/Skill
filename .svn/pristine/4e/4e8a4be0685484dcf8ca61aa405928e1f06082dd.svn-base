import { NgModule } from '@angular/core';
import { RouterModule, Routes } from '@angular/router';

import { LoginComponent } from './components/login/login.component';
import { RegisterComponent } from './components/register/register.component';
import { CompetencyListComponent } from './components/competency-list/competency-list.component';
// import { CompetencyFormComponent } from './components/competency-form/competency-form.component';



const routes: Routes = [
  { path: '', redirectTo: '/login', pathMatch: 'full' },
  { path: 'login', component: LoginComponent },
  { path: 'register', component: RegisterComponent },
  { path: 'competencies', component: CompetencyListComponent },
  // { path: 'competencies/new', component: CompetencyFormComponent },
  // { path: 'competencies/edit/:id', component: CompetencyFormComponent },
  // Wildcard
  { path: '**', redirectTo: '/login' }
];

@NgModule({
  imports: [RouterModule.forRoot(routes)],
  exports: [RouterModule]
})
export class AppRoutingModule {}
